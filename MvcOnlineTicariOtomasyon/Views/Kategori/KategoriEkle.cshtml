@model MvcOnlineTicariOtomasyon.Models.Siniflar.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-3.6.3.js"></script>
<br />
<h2>Yeni Kategori Sayfası</h2>
<br />
<form class="form-group" method="post">

    @Html.Label("Kategori Adı:")
    @Html.TextBoxFor(x => x.KategoriAd, new { @class = "form-control", @required = "required" })
    <br />
    <button class="btn btn-info">Kaydet</button>

    
</form>